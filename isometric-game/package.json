{
   "name": "isometric-game",
   "version": "1.0.0",
   "description": "An isometric game engine made with Electron",
   "main": "main.js",
   "scripts": {
      "start": "electron .",
      "build": "webpack --config webpack.config.js --mode production",
      "build.prod": "webpack --config webpack.config.js -p",
      "build.esnext": "npm run build:types && npm run build:js",
      "build:types": "tsc --emitDeclarationOnly",
      "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
      "build.all": "npm run build.prod && npm run build.esnext",
      "test": "karma start karma.config.js"
   },
   "author": "Thor Arisland",
   "license": "ISC",
   "devDependencies": {
      "@types/lodash": "^4.14.155",
      "electron": "^9.0.4",
      "jquery": "^3.5.1",
      "@types/jasmine": "2.8.7",
      "@types/node": "7.0.0",
      "awesome-typescript-loader": "5.2.0",
      "jasmine-core": "3.1.0",
      "karma": "2.0.4",
      "karma-jasmine": "1.1.2",
      "karma-phantomjs-launcher": "1.0.4",
      "karma-webpack": "3.0.0",
      "source-map-loader": "0.2.3",
      "tslint": "5.17.0",
      "tslint-loader": "3.6.0",
      "typescript": "3.5.1",
      "webpack": "4.12.0",
      "webpack-cli": "3.0.8",
      "webpack-dev-server": "3.1.4",
      "@babel/cli": "^7.2.3",
      "@babel/core": "^7.4.0",
      "@babel/plugin-proposal-class-properties": "^7.4.0",
      "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
      "@babel/preset-env": "^7.4.1",
      "@babel/preset-typescript": "^7.3.3"
   },
   "dependencies": {}
}
